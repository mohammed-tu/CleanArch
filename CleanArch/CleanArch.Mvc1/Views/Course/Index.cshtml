@model IEnumerable<CleanArch.Application.ViewModels.CourseViewModel>



<div class="container body-content" >

    <div class="row course-content" >

        @foreach (var course in Model)
        {
            <div class="course-image-container" style=" margin: 5px;">
                <div class="course-image" style="background-image:url(@course.ImageUrl) ;">ttttttttt</div>
            </div>

            <span> @course.Name  </span>
        }
    </div>
</div>